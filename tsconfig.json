{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["components/*"],
      "@svg": ["utils/SVG/index.tsx"],
      "@utils/*": ["utils/*"],
      "@wallet/*": ["lib/connect-wallet/*"],
      "@modal/*": ["lib/modal/*"],
      "@toast/*": ["lib/toast/*"],
      "@date/*": ["lib/date/*"],
      "@config/*": ["config/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "components/Whitelist/WhitelistTable.jsx"],
  "exclude": ["node_modules"]
}
